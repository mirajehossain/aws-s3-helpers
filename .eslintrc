{
  "rules": {
    "no-console": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "next"
      }
    ],

    "no-multi-str": 0,
    "no-return-await": "off",
    "no-param-reassign": [2, { "props": false }]
  },
  "env": {
    "node": true,
    "mocha": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 9
  },
  "extends": [
    "airbnb-base"
  ]
}
